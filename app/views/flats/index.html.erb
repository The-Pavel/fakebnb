<h1>Flats#index</h1>
<p>Find me in app/views/flats/index.html.erb</p>

<div class="container">
  <% @flats.each do |f| %>
    <div class="card" >
      <img src=<%= f['imageUrl'] %> alt="">
      <p><%= link_to f['name'], flat_path(f["id"]) %></p>
      <p><%= f['price'] %><%= f['priceCurrency'] %></p>
    </div>

  <% end %>


</div>
